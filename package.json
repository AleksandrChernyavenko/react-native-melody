{
  "name": "melody",
  "version": "0.1.0",
  "private": true,
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "dts": "./scripts/generate_cross_platform_types",
    "typecheck": "node_modules/.bin/tsc --pretty",
    "typewatch": "node_modules/.bin/tsc --pretty --watch",
    "pretest": "npm run typecheck && npm run lint",
    "test": "node node_modules/jest/bin/jest.js",
    "tdd": "node node_modules/jest/bin/jest.js --watch",
    "lint": "./node_modules/.bin/tslint -p . --format stylish"
  },
  "dependencies": {
    "expo": "^28.0.0",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-28.0.0.tar.gz",
    "react-native-size-matters": "^0.1.2",
    "react-navigation": "2.7.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@types/expo": "27.0.6",
    "@types/jest": "22.2.3",
    "@types/react": "16.3.14",
    "@types/react-native": "0.55.14",
    "@types/react-navigation": "2.0.11",
    "@types/react-redux": "^6.0.4",
    "@types/react-test-renderer": "16.0.1",
    "@types/redux-form": "^7.4.1",
    "jest-expo": "^28.0.0",
    "react-native-scripts-ts": "1.15.0",
    "react-native-typescript-transformer": "1.2.3",
    "react-test-renderer": "16.3.1",
    "ts-jest": "22.4.6",
    "tslib": "1.9.1",
    "tslint": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
