{
  "name": "melody",
  "version": "0.1.0",
  "private": true,
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "dts": "./scripts/generate_cross_platform_types",
    "typecheck": "node_modules/.bin/tsc --pretty",
    "typewatch": "node_modules/.bin/tsc --pretty --watch",
    "pretest": "npm run typecheck && npm run lint",
    "test": "node node_modules/jest/bin/jest.js",
    "tdd": "node node_modules/jest/bin/jest.js --watch",
    "lint": "./node_modules/.bin/tslint -p . --format stylish"
  },
  "dependencies": {
    "expo": "^29.0.0",
    "react": "^16.4.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-29.0.0.tar.gz",
    "react-native-size-matters": "^0.1.3",
    "react-navigation": "^2.12.1",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-form": "7.4.2",
    "redux-persist": "5.10.0",
    "redux-saga": "0.16.0"
  },
  "devDependencies": {
    "@types/expo": "^27.0.8",
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.13",
    "@types/react-native": "0.55.14",
    "@types/react-navigation": "^2.0.19",
    "@types/react-redux": "^6.0.7",
    "@types/react-test-renderer": "^16.0.2",
    "@types/redux-form": "^7.4.7",
    "jest-expo": "^29.0.0",
    "react-native-scripts-ts": "1.15.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "^16.4.2",
    "ts-jest": "^23.1.4",
    "tslib": "^1.9.3",
    "tslint": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
